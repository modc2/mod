{
  "name": "prefi-prediction-market",
  "version": "1.0.0",
  "description": "Zero-sum prediction market with modular price oracles on Base, Ethereum, and Ganache",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:ganache": "hardhat run scripts/deploy.js --network ganache",
    "deploy:base": "hardhat run scripts/deploy.js --network base",
    "deploy:baseGoerli": "hardhat run scripts/deploy.js --network baseGoerli",
    "deploy:mainnet": "hardhat run scripts/deploy.js --network mainnet",
    "deploy:goerli": "hardhat run scripts/deploy.js --network goerli",
    "verify:base": "hardhat verify --network base",
    "verify:mainnet": "hardhat verify --network mainnet",
    "ganache": "ganache-cli -p 7545 -i 5777",
    "node": "hardhat node"
  },
  "keywords": [
    "defi",
    "prediction-market",
    "base",
    "ethereum",
    "smart-contracts",
    "solidity"
  ],
  "author": "Mr. Robot",
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@truffle/hdwallet-provider": "^2.1.15",
    "chai": "^4.3.8",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "ganache-cli": "^6.12.2",
    "hardhat": "^2.17.2",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.4",
    "truffle": "^5.11.5",
    "truffle-plugin-verify": "^0.6.7"
  }
}
